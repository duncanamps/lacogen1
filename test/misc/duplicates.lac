%title          = "LaCoGen Test Multiple Keywords";
%version        = "1.0";
%author         = "Duncan Munro";
%codeprefix     = "TEST";
%start          = <Content>;


// ====================================================================
// Predefined Character sets
// ====================================================================

{WSChar}        = [\t\r\n ];

 
// ====================================================================
// Terminals
// ====================================================================

KTrue					= ("T"|"t") ("R"|"r") ("U"|"u") ("E"|"e") keyword;
KFalse					= ("F"|"f") ("A"|"a") ("L"|"l") ("S"|"s") ("E"|"e") keyword;

WhiteSpace				= {WSChar}+ ignore;


// ====================================================================
// Rules
// ====================================================================

// High level rules

<Content>			: KTrue					# ActIgnore
//					| KFalse					# ActIgnore
					;
