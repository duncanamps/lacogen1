%title        = "LaCoGen TEST SR Conflict";
%version      = "0";
%author       = "Duncan Munro";
%codeprefix   = "SRC";
%start        = <E>;


// ====================================================================
// Predefined Character sets
// ====================================================================

{nset}       = [0-9];
                                  
// ====================================================================
// Terminals
// ====================================================================

n = {nset}+;


// ====================================================================
// Rules
// ====================================================================

<E> : n
    | <E> "+" <E>
    | <E> "*" <E>
    | "(" <E> ")"
    ;
    
